{
  "expo": {
    "name": "second_project",
    "slug": "shreddersbay",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon2.png",
    "userInterfaceStyle": "light",
    "platforms": ["android", "ios"],
    "permissions": ["INTERNET"],
    "extra": {
      "eas": {
        "projectId": "899a3d51-4ab2-46d5-a140-644e3ad1b7be"
      }
    },
    "androidStatusBar": {
      "backgroundColor": "#ffffff",
      "barStyle": "dark-content",
      "translucent": true,
      "hidden": false
    },
    "splash": {
      "image": "./assets/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shreddersbay",
      "minVersion": "13.0",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["myapp"]
          }
        ]
      },
      "statusBar": {
        "backgroundColor": "#ffffff", 
        "style": "dark"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shreddersbay",
      "versionCode": 3
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "vinitrathore",
    "scheme": "second_project",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/899a3d51-4ab2-46d5-a140-644e3ad1b7be"
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 31,
            "targetSdkVersion": 31,
            "buildToolsVersion": "31.0.0"
          },
          "ios": {
            "deploymentTarget": "13.4",
            "useFrameworks": "static"
          }
        }
      ]
    ]
  }
}
